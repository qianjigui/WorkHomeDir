# This is a git configuration file.
[user]
    #name = qianjigui
    #email = qianjigui@gmail.com
[color]
    ui = auto
[alias]
    co = checkout
    ci = commit
    st = status
    br = branch
    ll = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr,%an)%Creset' --abbrev-commit --date=relative
[commit]
    template = ~/.gittemplate
[core]
	editor = /usr/bin/vim
	autocrlf = input
[http]
	postBuffer = 524288000
